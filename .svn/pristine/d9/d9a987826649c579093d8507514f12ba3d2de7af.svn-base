// pages/personal/money/money.js
const app = getApp();
Page({

	/**
	 * 页面的初始数据
	 */
	data: {
		usermoney:null,
		freeze:null,
		pay_points:null,
		nvabarData: {
          showCapsule: 1,  //1表示显示    0表示不显示
          title: '我的余额',   // 名片
          type: '1'
        },
        height: app.globalData.height * 2
	},

	/**
	 * 生命周期函数--监听页面加载
	 */
	onLoad: function (options) {
		this.setData({
			usermoney: options.usermoney,
			freeze: options.freeze,
			pay_points: options.pay_points
		})
	},

	
})