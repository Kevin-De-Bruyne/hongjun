/**/
.protocol{
    display: flex;
    align-items: center;
    height: 44px;
    padding: 0 7px;
}
.isprotocol{
    height: 20px;
    width: 20px;
    border: #e1e1e1 solid 1px;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 3px;
}
.protocolme{
    font-weight: bold;
    font-size: 14px;
    margin-left: 7px;
}
.protocoluser{
    font-size: 12px;
    font-weight: 500;
    color: #8bc34a;
}

/**/
.typestatus{
	position: fixed;
	left: 0;
	width: 100%;
	height: 100%;
	background-color: #000000b5;
	z-index: 10;
	color: red;
	text-align: center;
	line-height: 500rpx;
}
.descsgo{
	font-size: 14px;
	padding: 7px 12px;
	position: fixed;
	left: 0;
	z-index: 10;
	height: 60px;
	background-color: #EDEDED;
	display: flex;
	width: 100%;
}
.descsgos{
	line-height: 20px;
	text-align: left;
	height: 60px;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 3;
	-webkit-box-orient: vertical;
	overflow: hidden;
	color: red;
	white-space:normal;
	width: 73%;
}