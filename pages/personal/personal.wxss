.avatar{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	align-items:center;
	flex-direction: row;
	/*background: #8bc34a;*/
	color: #fff;
	font-size: 12px;
	padding: 17px 15px;
}
.avatarUrl{
	width: 60px;
	height: 60px;
	border-radius: 7px;
}
.avatar_btn{
	width: 120rpx;
	height: 45rpx;
	border-radius: 7rpx;
	border: 1px solid white;
	text-align: center;
	line-height: 45rpx;
	background-color: #fff;
	color: black;
	margin-left: 25rpx;
	/* font-size: 20rpx; */
}
.avatar .xieyi{
	display: flex;
	margin-top: 25px;
	flex-direction: row;
}
.xieyi checkbox{
	width: 60rpx;
	height: 60rpx;
}
.avatar_btn button{
padding: 0;
margin: 0;
width: 100%;
height: 100%;
font-size: 23rpx;
line-height: 45rpx;
background-color: #fff;
}
